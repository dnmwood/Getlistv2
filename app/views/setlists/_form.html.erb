<div class="container">
  <div class="row">
    <div class="col-sm-4 text-center col-md-offset-4">

      <h1>New Setlist</h1>
        <%= form_for [@setlist] do |f| %>
          <%= f.label :title %>
          <%= f.text_field :title, :class => "form-control" %>

          <%= f.submit "Submit", :class=>"btn btn-primary" %>
        <% end %>

    </div>
  </div>
</div>
